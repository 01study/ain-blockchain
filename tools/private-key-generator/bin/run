// Execute this node script to return a new valid private key.
// This private key can be used as an argument for the blockchain-database 
// by specifing PRIVATE_KEY=<private_key> as a command line argument.

const EC = require('elliptic').ec;
const SHA256 = require('crypto-js/sha256');
const ec = new EC('secp256k1');
console.log(ec.genKeyPair().getPrivate().toString(16));
