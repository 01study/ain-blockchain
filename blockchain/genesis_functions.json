{
  "deposit": {
    "$service": {
      "$user_addr": {
        "$deposit_id": {
          "value": {
            ".function": {
              "function_type": "NATIVE",
              "function_id": "_deposit"
            }
          }
        }
      }
    }
  },
  "sharding": {
    "shard": {
      "$sharding_path": {
        ".function": {
          "function_type": "NATIVE",
          "function_id": "_initializeShard"
        }
      }
    }
  },
  "transfer": {
    "$from": {
      "$to": {
        "$key": {
          "value": {
            ".function": {
              "function_type": "NATIVE",
              "function_id": "_transfer"
            }
          }
        }
      }
    }
  },
  "withdraw":{
    "$service": {
      "$user_addr": {
        "$withdraw_id": {
          "value": {
            ".function": {
              "function_type": "NATIVE",
              "function_id": "_withdraw"
            }
          }
        }
      }
    }
  }
}
